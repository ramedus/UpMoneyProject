@model UpMoneyProjesi.Models.MySubscribe

@{
    ViewData["Title"] = "CreateSubscribe";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>CreateSubscribe</h1>

<h4>MySubscribe</h4>
<hr />
<div class="row" style="margin-left: 288px; margin-top: 91px; >
    <div class="col-md-4">
        <form asp-action="CreateSubscribe">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="SubscribeTypeId" class="control-label"></label>
                <select asp-for="SubscribeTypeId" class ="form-control" asp-items="ViewBag.SubscribeTypeId"></select>
            </div>
            <div class="form-group">
                <label asp-for="SubscribeValue" class="control-label"></label>
                <input asp-for="SubscribeValue" class="form-control" />
                <span asp-validation-for="SubscribeValue" class="text-danger"></span>
            </div>
           
            <div class="form-group">
                @Html.HiddenFor(model => model.SubscribeId)
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

